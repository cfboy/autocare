<!-- Alerts Code -->
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.3.3/dist/sweetalert2.all.min.js"></script>
<script type="text/javascript" src="../js/sweet-alerts.js"></script>
<script>
    $(document).ready(function() {
        var msj = '<%- message %>';
        var alertType = '<%- alertType %>';
        var allAlertTypes = '<%- JSON.stringify(alertTypes) %>';

        if (msj !== '' && alertType !== '') {
            sweetAlerts(allAlertTypes, alertType, msj);
        }
    });
</script>